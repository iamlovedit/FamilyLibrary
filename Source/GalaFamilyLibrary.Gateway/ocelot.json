{
  "Routes": [
    {
      "UpstreamPathTemplate": "/package/{url}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Delete",
        "Put"
      ],
      "DownstreamPathTemplate": "/{url}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 6001
        }
      ],
//      "AuthenticationOptions": {
//        "AuthenticationProviderKey": "Bearer",
//        "AllowedScopes": []
//      },
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "1s",
        "PeriodTimespan": 5,
        "Limit": 1
      }
    },
    {
      "UpstreamPathTemplate": "/family/{url}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Delete",
        "Put"
      ],
      "DownstreamPathTemplate": "/{url}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7001
        }
      ],
//      "AuthenticationOptions": {
//        "AuthenticationProviderKey": "Bearer",
//        "AllowedScopes": []
//      },
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "1s",
        "PeriodTimespan": 5,
        "Limit": 1
      }
    },
    {
      "UpstreamPathTemplate": "/user/{url}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Delete",
        "Put"
      ],
      "DownstreamPathTemplate": "/{url}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 8001
        }
      ],
//      "AuthenticationOptions": {
//        "AuthenticationProviderKey": "Bearer",
//        "AllowedScopes": []
//      },
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "1s",
        "PeriodTimespan": 5,
        "Limit": 1
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:5001",
    "RouteIsCaseSensitive": false,
    "DangerousAcceptAnyServerCertificateValidator": true
  }
}